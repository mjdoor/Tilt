{
  "expo": {
    "name": "Tilt",
    "slug": "Tilt",
    "description": "A fun, simple, tilty, memory game.",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.mdooreleyers.tilt",
      "versionCode": 2,
      "permissions": ["VIBRATE"]
    }
  }
}
